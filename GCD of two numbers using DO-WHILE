class Box{  
double length;
double breadth;  
double height;
void setLength(double l){  
length=l;
}
void setBreadth(double b){  
breadth=b;
}
void setHeight(double h){  
height=h;
}
void getVolume(){  
System.out.println("Volume of the Box is : "+(length*breadth*height));
}
}
public class BoxMain {
public static void main(String[] args) {  
Box b=new Box();  
b.setLength(12);
b.setBreadth(11);  
b.setHeight(10);  
b.getVolume();
}
}
